    "moment": "^2.29.4",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "nprogress": "^0.2.0",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.1.2"
